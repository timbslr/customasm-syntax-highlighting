{
	"name": "customasm-toolbox",
	"displayName": "customasm-toolbox",
	"description": "",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.105.0"
	},
	"categories": [
		"Programming Languages"
	],
	"publisher": "CodingT",
	"activationEvents": [],
	"main": "./out/extension.js",
	"repository": {
		"type": "git",
		"url": "git://github.com/timbslr/customasm-toolbox.git"
	},
	"contributes": {
		"commands": [
			{
				"command": "customasm-toolbox.helloWorld",
				"title": "Hello World"
			}
		],
		"languages": [
			{
				"id": "customasm-assembly",
				"aliases": [
					"customasm-assembly",
					"customasm-assembly"
				],
				"extensions": [
					".asm"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "customasm-assembly",
				"scopeName": "source.customasm-assembly",
				"path": "./src/syntaxes/customasm-assembly.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.19.1",
		"@types/vscode": "^1.106.0",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.36.0",
		"typescript": "^5.9.3"
	}
}
